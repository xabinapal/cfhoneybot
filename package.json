		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"@tailwindcss/postcss": "^4.1.12",
		"@types/node": "^22.17.2",
		"kysely": "^0.28.5",
		"kysely-d1": "^0.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.38.2",
		"tailwindcss": "^4.1.12",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"vite": "^7.1.3",
		"vite-plugin-devtools-json": "^1.0.0",
		"wrangler": "^4.31.0"
